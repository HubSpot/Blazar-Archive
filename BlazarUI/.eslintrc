{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "templateStrings": true,
    "jsx": true,
    "restParams": true,
    "spread": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "camelcase": 0,
    "no-alert": 0,
    "no-else-return": 1,
    "strict": [2, "global"],
    "brace-style": [2, "stroustrup"],
    "comma-style": [1, "last"],
    "no-lonely-if": 1,
    "no-multi-spaces": 0,
    "no-mixed-requires": 2,
    "no-underscore-dangle": 0,
    "no-var": 2,
    "no-duplicate-case": 0,
    "no-func-assign": 0,
    "quotes": [1, "single", "avoid-escape"],
    "space-before-function-paren": [1, "never"],
    "space-after-keywords": [1, "always"],
    "space-before-blocks": [1, "always"],
    "space-in-brackets": 0,
    "space-in-parens": 0,
    "spaced-comment": 0,
    "new-cap": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "new-parens": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-lonely-if": 0,
    "semi": [2, "always"]
  },
  "globals": {
    "__BROWSER__": false,
    "__SERVER__": false,
    "__DEV__": false,
    "__PROD__": false,
    "__": false,
    "Promise": true,
    "casper": false,
    "jest": false
  }
}
